{
    "files.associations": {
        "*.h": "c",
        "*.hpp": "cpp",
        "*.cpp": "cpp",
        "*.c": "c",
        "*.json": "json"
    },
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.compilerPath": "/usr/bin/clang++",
    "C_Cpp.default.intelliSenseMode": "macos-clang-x64",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/agent",
        "${workspaceFolder}/src/manager",
        "${workspaceFolder}/src/utils",
        "${workspaceFolder}/third_party/nlohmann_json/include",
        "${workspaceFolder}/third_party/cpp-httplib",
        "${workspaceFolder}/third_party/SQLiteCpp/include",
        "${workspaceFolder}/third_party/sqlite3/include",
        "${workspaceFolder}/third_party/spdlog/include",
        "${workspaceFolder}/third_party/uuid"
    ],
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.warningSquiggles": "enabled",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.autocomplete": "default",
    "C_Cpp.enhancedColorization": "enabled",
    "C_Cpp.suggestSnippets": true,
    "C_Cpp.autoAddFileAssociations": true,
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src",
        "${workspaceFolder}/third_party"
    ],
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.cwd": "${workspaceFolder}",
    "files.exclude": {
        "**/build": true,
        "**/*.o": true,
        "**/*.d": true
    },
    "search.exclude": {
        "**/build": true,
        "**/third_party": false
    },
    "files.watcherExclude": {
        "**/build/**": true
    }
}